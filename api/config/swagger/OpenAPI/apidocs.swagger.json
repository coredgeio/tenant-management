{
  "swagger": "2.0",
  "info": {
    "title": "tenant management API spec",
    "version": "1.0"
  },
  "tags": [
    {
      "name": "TenantMgmtApi"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/payment-config": {
      "get": {
        "summary": "get payment configuration status",
        "operationId": "TenantMgmtApi_GetPaymentConfigStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/configPaymentConfigStatusResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "TenantMgmtApi"
        ]
      }
    },
    "/v1/tenant-kyc": {
      "get": {
        "summary": "get tenant level kyc status",
        "operationId": "TenantMgmtApi_GetTenantKycStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/configTenantKycStatusResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "TenantMgmtApi"
        ]
      }
    },
    "/v1/tenant-type": {
      "get": {
        "summary": "get tenant type",
        "operationId": "TenantMgmtApi_GetTenantType",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/configTenantTypeResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "TenantMgmtApi"
        ]
      }
    },
    "/v1/tenant-user-kyc": {
      "get": {
        "summary": "get tenant user level kyc status",
        "operationId": "TenantMgmtApi_GetTenantUserKycStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/configTenantUserKycStatusResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "TenantMgmtApi"
        ]
      }
    }
  },
  "definitions": {
    "configKycStatus": {
      "type": "string",
      "enum": [
        "Unspecified",
        "Pending",
        "InProcess",
        "Failed",
        "Partial",
        "Done",
        "ReKycNeeded"
      ],
      "default": "Unspecified",
      "title": "- Unspecified: Default value, unspecified status\n - Pending: KYC pending\n - InProcess: Kyc in process\n - Failed: kyc failed\n - Partial: kyc partial\n - Done: kyc done successfully\n - ReKycNeeded: re-kyc is required"
    },
    "configPaymentConfigStatusResp": {
      "type": "object",
      "properties": {
        "isPayMethodSet": {
          "type": "boolean",
          "title": "payment method configured or not"
        }
      }
    },
    "configTenantKycStatusResp": {
      "type": "object",
      "properties": {
        "isKycDone": {
          "type": "boolean",
          "title": "kyc status in bool"
        },
        "kycStatus": {
          "$ref": "#/definitions/configKycStatus",
          "title": "kycStatus in enum"
        }
      }
    },
    "configTenantType": {
      "type": "string",
      "enum": [
        "Unknown",
        "Individual",
        "Organization"
      ],
      "default": "Unknown",
      "title": "- Unknown: unknown tenant type\n - Individual: individual tenant type\n - Organization: organization tenant type"
    },
    "configTenantTypeResp": {
      "type": "object",
      "properties": {
        "tenantType": {
          "$ref": "#/definitions/configTenantType",
          "title": "tenant type"
        }
      }
    },
    "configTenantUserKycStatusResp": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "title": "email under consideration"
        },
        "username": {
          "type": "string",
          "title": "username under consideration"
        },
        "isKycDone": {
          "type": "boolean",
          "title": "kyc status in bool"
        },
        "kycStatus": {
          "$ref": "#/definitions/configKycStatus",
          "title": "kycStatus in enum"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  },
  "externalDocs": {
    "description": "Tenant Management API spec, sample"
  }
}
